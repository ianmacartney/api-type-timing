{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": false,
    "incremental": false,
    "diagnostics": true,
    "extendedDiagnostics": true
  },
  "include": ["src/**/*"]
}
